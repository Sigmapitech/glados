import math

fn nth_prime(k: int) -> int {
  if (k < 1) {return -1;};
  if (k == 1) {
    return 2;
  };
  if (k == 2) {
    return 3;
  };

  n: int = 3;
  is_prime: bool;

  k -= 2;
  while (k > 0) {
    n += 2;
    is_prime = True;

    for (i: int = 3; i < int(math.sqrt(n)) + 1; i++) {
      if ((n%i) == 0) {
        is_prime = False ;
      };
    };

    if (is_prime) {
      k--;
    };
  };
  return n;
}

fn main() -> int {
  for (i: int = 1; i < 21; i++) {
    print(nth_prime(i));
  };
}
